echo -e "${CYAN}Ejecutando tests unitarios y de integración…${NA}"
if ! make phpunit; then
  echo -e "${RED}Error al ejecutar tests unitarios y de integración${NA}"
  pilotes
fi

echo -e "${CYAN}Ejecutando PHPStan al nivel 8…${NA}"
if ! make phpstan; then
  echo -e "${RED}Error al pasar PHPStan${NA}"
  pilotes
fi

echo -e "${CYAN}Ejecutando PHP Code Sniffer…${NA}"
if ! make phpcs; then
  echo -e "${RED}Error al pasar PHP Code Sniffer${NA}"
  pilotes
fi

oumama

